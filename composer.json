{
  "name": "ccu-plus/attachment",
  "description": "Provide interface for core module to upload and scan attachments.",
  "keywords": ["ccu-plus"],
  "version": "0.0.1",
  "license": "AGPL-3.0-only",
  "type": "library",
  "authors": [
    {
      "name": "bepsvpt",
      "email": "og7lsrszah6y3lz@infinitefa.email"
    }
  ],
  "require": {
    "php": "^7.2",
    "guzzlehttp/guzzle": "^6.5",
    "illuminate/database": "^6.2",
    "illuminate/support": "^6.2"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^8.4"
  },
  "autoload": {
    "psr-4": {
      "CCUPLUS\\Attachment\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CCUPLUS\\Attachment\\Tests\\": "tests/"
    },
    "classmap": [
      "tests"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
